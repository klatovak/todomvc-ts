{
  "compilerOptions": {
    "plugins": [
      {
        "name": "tslint-language-service"
      }
    ],

    "target": "es6",
    "module": "es2015",

    "jsx": "react",
    "rootDir": "src",

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,

    "inlineSourceMap": true,
    "inlineSources": true
  }
}
